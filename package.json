{
  "name": "nuxt-daisy",
  "version": "0.1.0",
  "description": "Nuxt3 Starter with DaisyUI",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --cache --write \"**/*.{cjs,js,jsx,ts,tsx,vue}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint && yarn coverage"
  },
  "dependencies": {
    "pinia": "^2.0.24"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@fontsource/fira-mono": "^4.5.10",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/poppins": "^4.5.10",
    "@nuxt/test-utils-edge": "^3.0.1-rc.0-27810184.d991a55",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@pinia/nuxt": "^0.4.4",
    "@pinia/testing": "^0.0.14",
    "@tailwindcss/typography": "^0.5.8",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitest/coverage-c8": "^0.25.2",
    "@vueuse/nuxt": "^9.5.0",
    "c8": "^7.12.0",
    "daisyui": "^2.41.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^9.7.0",
    "nuxt": "~3.0.0",
    "nuxt-server-fn": "^0.3.0",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "~4.9.3",
    "vitest": "^0.25.2"
  }
