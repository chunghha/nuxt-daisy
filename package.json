{
  "name": "nuxt-daisy",
  "version": "0.1.0",
  "description": "Nuxt3 Starter with DaisyUI",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
